<UserControl x:Class="STI.PixisDeviceUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:STI"
             mc:Ignorable="d" 
             d:DesignHeight="305" d:DesignWidth="271"
             HorizontalContentAlignment="Stretch" HorizontalAlignment="Left">
    <Grid HorizontalAlignment="Left">
        <Button x:Name="connectButton" Content="Connect" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="connect_button_Click"/>
        <Label x:Name="aquireStatusLabel" Content="Label" HorizontalAlignment="Left" Margin="10,50,-176.2,0" VerticalAlignment="Top" Width="251"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="155" Margin="10,95,-176.2,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="251"/>
        <Button x:Name="disconnectButton" Content="Disconnect" HorizontalAlignment="Left" Margin="101,10,-91.2,0" VerticalAlignment="Top" Width="75" Click="disconnectButton_Click"/>
        <Button x:Name="testButton" Content="test" HorizontalAlignment="Left" Margin="10,264,-0.2,0" VerticalAlignment="Top" Width="75" Click="testButton_Click"/>
    </Grid>
</UserControl>
